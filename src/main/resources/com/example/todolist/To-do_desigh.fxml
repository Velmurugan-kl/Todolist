<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.todolist.HelloController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="555.0" layoutY="289.0" style="&#10;" text="myfx" textAlignment="CENTER" textFill="#9e9e9e00" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <Button fx:id="done" layoutX="531.0" layoutY="89.0" mnemonicParsing="false" onAction="#ondone" prefHeight="27.0" prefWidth="86.0" text="Done" />
            <Button fx:id="add" layoutX="530.0" layoutY="30.0" mnemonicParsing="false" onAction="#onHelloButtonClick" prefHeight="26.0" prefWidth="86.0" text="Add" />
            <ListView fx:id="welcomeText" layoutX="14.0" layoutY="89.0" prefHeight="244.0" prefWidth="504.0" />
            <TextField fx:id="txt" layoutX="14.0" layoutY="30.0" prefHeight="26.0" prefWidth="504.0" promptText="Enter the task" />
      </children>
    </AnchorPane>
  </children>
</VBox>
